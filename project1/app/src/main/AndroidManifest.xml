<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Freelance_p1"
        tools:targetApi="31">
        <activity
            android:name=".UI.CartActivity"
            android:exported="false" />
        <activity
            android:name=".UI.LoginAdmin"
            android:exported="false"
            android:label="@string/title_activity_login_admin"
            android:noHistory="true"
            android:theme="@style/Theme.Freelance_p1" />
        <activity
            android:name=".UI.SplashScreenActivtiy"
            android:exported="true"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.admin_add_data"
            android:exported="true"
            android:noHistory="true" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="false"
            android:noHistory="true" />
    </application>

</manifest>